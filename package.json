    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios15": "react-native run-ios --simulator 'iPhone 15 Pro'",
    "reset": "yarn start --reset-cache",
    "bundle": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "rename": "npx react-native-rename 'walletapp' -b 'com.dun.walletapp'",
    "asset": "npx react-native-asset",
    "cp:ios:stag": "appcenter codepush release-react -a duntd.dev-gmail.com/walletapp -d Staging",
    "cp:ios:prod": "appcenter codepush release-react -a duntd.dev-gmail.com/walletapp -d Production",
    "cp:android:stag": "appcenter codepush release-react -a duntd.dev-gmail.com/DunDaily -d Staging",
